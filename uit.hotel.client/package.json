{
    "name": "uit.hotel.client",
    "version": "1.0.0",
    "description": "Nuxt.js project",
    "author": "Phạm Hồng Phúc <phamhongphuc.info@gmail.com>",
    "private": true,
    "scripts": {
        "dev": "nuxt-ts",
        "dev:s": "cd ../uit.hotel && dotnet run",
        "build": "nuxt-ts build",
        "build:s": "tsc nuxt.config.ts",
        "start": "nuxt-ts start",
        "heroku-postbuild": "yarn build",
        "precommit": "yarn lint",
        "gql": "graphql-codegen --config codegen.yml",
        "prettier:fix": "prettier ** --write",
        "prettier:fix:w": "prettier ** --write --end-of-line crlf",
        "lint": "run-s lint:*",
        "lint:js": "eslint --ext .ts,.js,.vue,.gql,.graphql .",
        "lint:css": "stylelint **/*.{vue,scss}",
        "lint:css:fix": "stylelint **/*.{vue,scss} --fix",
        "type:fix": "run-s prettier:fix \"lint:* --fix\"",
        "test": "mocha test/**/*.ts -r test/ts-register --exit"
    },
    "dependencies": {
        "@nuxt/typescript-runtime": "^0.3.1",
        "@nuxtjs/apollo": "^4.0.0-rc18",
        "@nuxtjs/style-resources": "^1.0.0",
