        "@graphql-codegen/schema-ast": "^1.9.0",
        "@graphql-codegen/typescript": "^1.9.0",
        "@graphql-codegen/typescript-compatibility": "^1.9.0",
        "@graphql-codegen/typescript-operations": "^1.9.0",
        "@nuxt/types": "^0.5.5",
